import java.util.Scanner;

public class Main {
	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		long N = sc.nextLong();
		sc.close();
		
		//System.out.println(b_search(N,2));
		System.out.println(Math.abs(b_search(N,1)));

	}

	public static long b_search(long N, long x) {
		
		for(int i = 0; i < 1000; i++) {
			x = (x + (N/x)) / 2;
			
		}
		return x;
	}
	
}
